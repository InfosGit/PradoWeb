	<queryString>
		<![CDATA[SELECT
        C.NOM_GERENCIA,
        D.NOM_DEPTO,
        SUBSTR(TO_CHAR(B.MES_NUMERO, '00') || ' ' || B.MES_LETRA, 0, 7) MES,
        ROUND(NVL(COUNT(*), 0)/2) TOTAL
FROM
        EMPLEADOS A,
        MESES B,
        GERENCIA C,
        DEPARTAMENTOS D,
        PLANILLA E
WHERE
                C.COD_CIA = DECODE($P{CIA}, 0, C.COD_CIA, $P{CIA})
        AND     C.COD_GERENCIA = DECODE($P{GIA}, 0, C.COD_GERENCIA, $P{GIA})
        AND     D.COD_CIA = C.COD_CIA
        AND     D.COD_DEPTO = DECODE($P{DEPTO}, 0, D.COD_DEPTO, $P{DEPTO})
        AND     D.COD_GERENCIA = C.COD_GERENCIA
        AND     A.COD_CIA = D.COD_CIA
        AND     A.COD_DEPTO = D.COD_DEPTO
        AND     E.COD_CIA = A.COD_CIA
        AND     E.COD_EMP = A.COD_EMP
        AND     E.ANIO = $P{ANIO}
        AND     E.MES = B.MES_NUMERO
        AND     E.COD_TIPOPLA = 0
GROUP BY
        C.NOM_GERENCIA,
        D.NOM_DEPTO,
        SUBSTR(TO_CHAR(B.MES_NUMERO, '00') || ' ' || B.MES_LETRA, 0, 7)
ORDER BY 1, 2, 3]]>
