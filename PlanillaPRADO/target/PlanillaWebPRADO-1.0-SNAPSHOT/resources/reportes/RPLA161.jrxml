	<parameter name="CIA" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="ANIO" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT
        1 ORDEN,
        'INICIAL' CONCEPTO,
        TO_CHAR(B.MES_NUMERO, '00') MES,
        SUM(DECODE(GREATEST(ADD_MONTHS(TO_DATE('0101'||$P{ANIO},'DDMMYYYY'),B.MES_NUMERO-1)-1, A.FEC_INGRESO),
        ADD_MONTHS(TO_DATE('0101'||$P{ANIO},'DDMMYYYY')-1,B.MES_NUMERO-1),1,0))-(SELECT NVL(COUNT(*),0)
FROM
        EMPLEADOS
WHERE
                FEC_SALIDA < TO_DATE('01'||TO_CHAR(B.MES_NUMERO,'00')||TO_CHAR($P{ANIO},'0000'),'DDMMYYYY')
        AND     COD_CIA = DECODE($P{CIA}, 0, COD_CIA, $P{CIA})) CANTIDAD
FROM
        EMPLEADOS A,
        MESES B
WHERE
                COD_CIA = DECODE($P{CIA}, 0, COD_CIA, $P{CIA})
        AND     TO_CHAR(FEC_INGRESO,'YYYY') <= $P{ANIO}
	and b.MES_NUMERO < 13
GROUP BY
        B.MES_NUMERO

UNION ALL

SELECT
        2 ORDEN,
        'ALTAS' CONCEPTO,
        TO_CHAR(TO_CHAR(FEC_INGRESO,'MM'), '00') MES,
        NVL(COUNT(*),0) ALTAS
FROM
        EMPLEADOS
WHERE
                TO_CHAR(FEC_INGRESO,'YYYY') = $P{ANIO}
        AND     COD_CIA = DECODE($P{CIA}, 0, COD_CIA, $P{CIA})
GROUP BY
        TO_CHAR(FEC_INGRESO,'MM')

UNION ALL

SELECT
        3 ORDEN,
        'BAJAS' CONCEPTO,
        TO_CHAR(TO_CHAR(FEC_SALIDA,'MM'), '00') MES,
        NVL(-COUNT(*),0) BAJAS
FROM
        EMPLEADOS
WHERE
                TO_CHAR(FEC_SALIDA,'YYYY') = $P{ANIO}
        AND     COD_CIA = DECODE($P{CIA}, 0, COD_CIA, $P{CIA})
GROUP BY
        TO_CHAR(FEC_SALIDA,'MM')
ORDER BY 1, 2, 3]]>
	</queryString>
