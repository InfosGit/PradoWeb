SELECT
        A.COD_CIA,
        A.ANIO,
        B.MES_NUMERO,
        TO_CHAR(B.MES_NUMERO, '00') || '-' || (SUBSTR(B.MES_LETRA,0, 3)||'-'||SUBSTR(TO_CHAR($P{ANIO}),3)) MES,
        NVL(COUNT(*)/2,0) CANTIDAD,
        --NVL(SUM (A.TOT_DEBENGA),0) CANTIDAD,
        --NVL(SUM((NVL(A.CH_XSENCILLA,0)+NVL(A.CH_XDOBLE,0)+NVL(A.CH_X250,0)+NVL(A.CH_X150,0) + NVL(A.D_NOCTURNIDAD,0))),0) CANTIDAD,
        --NVL(SUM((NVL(A.VH_XSENCILLA,0) + NVL(A.VH_XDOBLE,0) +NVL(A.VH_X250,0)+NVL(A.VH_X150,0)+ NVL(A.CH_HD_NOCTURNIDAD,0))),0) CANTIDAD,
        'TOTAL PERSONNEL' DESCRIPCION
        --'TOTAL PAYROLL' DESCRIPCION
        --'HOURS OVERTIME' DESCRIPCION
        --'TOTAL OUTSOURCING' DESCRIPCION
FROM
        PLANILLA A,
        MESES B,
        CIAS C
WHERE
                A.COD_CIA = DECODE($P{CIA}, 0, A.COD_CIA, $P{CIA})
        AND     A.ANIO = $P{ANIO}
        AND     A.MES = B.MES_NUMERO
        AND     A.COD_CIA = C.COD_CIA
        AND     A.FECHA <= C.FEC_INICIO_OPERACION
GROUP BY
        A.COD_CIA,
        A.ANIO,
        B.MES_NUMERO,
        B.MES_LETRA
UNION ALL
