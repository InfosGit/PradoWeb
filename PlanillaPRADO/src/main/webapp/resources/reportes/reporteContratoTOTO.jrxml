	<queryString>
		<![CDATA[select
        p.cod_cia,
        p.cod_rep_patronal,
        c.cod_contrato,

        e.nombres || ' ' || e.apellidos as nombre_empl,
        trunc(sysdate - e.fecha_nac) as edad_empl,
        decode(e.sexo, 1, 'masculino', 2, 'femenino') as sexo_empl,
        ece.nom_estado_civil as estado_civil_empl,
        pre.nom_profesion as profesion_empl,
        nvl(e.direccion, '[no disponible]') as direccion_empl,
        pe.nacionalidad as nacionalidad_empl,
        e.num_dui as dui_empl,

        j.nombres || ' ' || j.apellidos as nombre_jefe,
        trunc(sysdate - j.fecha_nac) as edad_jefe,
        decode(j.sexo, 1, 'masculino', 2, 'femenino') as sexo_jefe,
        ecj.nom_estado_civil as estado_civil_jefe,
        prj.nom_profesion as profesion_jefe,
        nvl(j.direccion, '[no disponible]') as direccion_jefe,
        pj.nacionalidad as nacionalidad_jefe,
        j.num_dui as dui_jefe,

        ci.nom_comercial as nombre_empresa,
        c.fecha_inicio as inicio_contrato,
        c.fecha_final as fin_contrato,
        pue.nom_puesto as puesto_empl,
        c.salario as salario_empl,
        importeenletras(c.salario/30.00) as sal_letras,
        c.fecha_acuerdo as fecha_contrato
from
        cias ci,
        param_plan p,
        contrato c,

        empleados e,
        estado_civil ece,
        paises pe,
        profesion pre,
        puestos pue,

        empleados j,
        estado_civil ecj,
        paises pj,
        profesion prj
where
                ci.cod_cia = $P{COD_CIA}

        and     p.cod_cia = ci.cod_cia

        and     c.cod_cia = p.cod_cia
        and     c.cod_contrato = $P{COD_CONTRATO}

        and     e.cod_cia = p.cod_cia
        and     e.cod_emp = c.cod_emp

        and     ece.cod_estado_civil = e.estado_civil
        and     pe.cod_pais = e.cod_pais_nacionalidad

        and     pre.cod_cia = p.cod_cia
        and     pre.cod_profesion = e.cod_profesion

        and     pue.cod_cia = p.cod_cia
        and     pue.cod_puesto = c.cod_puesto

        and     j.cod_cia = p.cod_cia
        and     j.cod_emp = p.cod_rep_patronal

        and     ecj.cod_estado_civil = j.estado_civil
        and     pj.cod_pais = j.cod_pais_nacionalidad

        and     prj.cod_cia = p.cod_cia
        and     prj.cod_profesion = j.cod_profesion]]>
	</queryString>
